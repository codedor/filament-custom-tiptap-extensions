{
    "name": "codedor/filament-custom-tiptap-extensions",
    "description": "Some custom extensions for Filament Tiptap",
    "keywords": [
        "codedor",
        "laravel",
        "filament-custom-tiptap-extensions"
    ],
    "homepage": "https://github.com/codedor/filament-custom-tiptap-extensions",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "awcodes/filament-tiptap-editor": "^3.5",
        "codedor/filament-link-picker": "^1.6",
        "codedor/filament-media-library": "^1.0|^2.0|^3.0",
        "filament/filament": "^3.1",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "larastan/larastan": "^2.0|^3.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "phpstan/extension-installer": "^1.1|^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.0|^2.0",
        "phpstan/phpstan-phpunit": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Codedor\\FilamentCustomTiptapExtensions\\": "src",
            "Codedor\\FilamentCustomTiptapExtensions\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codedor\\FilamentCustomTiptapExtensions\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedor\\FilamentCustomTiptapExtensions\\Providers\\FilamentCustomTiptapExtensionsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
