{
    "name": "codedor/filament-custom-tiptap-extensions",
    "description": "Some custom extensions for Filament Tiptap",
    "keywords": [
        "codedor",
        "laravel",
        "filament-custom-tiptap-extensions"
    ],
    "homepage": "https://github.com/codedor/filament-custom-tiptap-extensions",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "awcodes/filament-tiptap-editor": "^3.2",
        "codedor/filament-link-picker": "^0.1",
        "codedor/filament-media-library": "^0.3",
        "illuminate/contracts": "^10.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Codedor\\FilamentCustomTiptapExtensions\\": "src",
            "Codedor\\FilamentCustomTiptapExtensions\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codedor\\FilamentCustomTiptapExtensions\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedor\\FilamentCustomTiptapExtensions\\Providers\\FilamentCustomTiptapExtensionsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
